{
  "name": "@omeratt/rn-net-vision",
  "version": "0.2.3",
  "description": "A lightweight native module for React Native that provides real-time network monitoring and debugging through an intuitive web interface. Inspect HTTP/HTTPS requests, analyze responses, and troubleshoot API issues with ease",
  "source": "src",
  "main": "./lib/module/index.js",
  "types": "./lib/typescript/src/index.d.ts",
  "type": "commonjs",
  "exports": {
    ".": {
      "types": "./lib/typescript/src/index.d.ts",
      "default": "./lib/module/index.js"
    },
    "./package.json": "./package.json",
    "./metro": "./metro/index.js",
    "./server": "./server/start-debugger.js"
  },
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "metro",
    "server",
    "web-viewer/dist",
    "react-native.config.js",
    "!web-viewer/src",
    "!web-viewer/node_modules",
    "*.podspec",
    "!ios/build",
    "!android/build",
    "!android/gradle",
    "!android/gradlew",
    "!android/gradlew.bat",
    "!android/local.properties",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.*"
  ],
  "bin": "./server/start-debugger.js",
  "scripts": {
    "example": "yarn workspace @omeratt/rn-net-vision-example",
    "test": "jest",
    "typecheck": "tsc",
    "lint": "eslint . --ext .js,.ts,.tsx --ignore-pattern web-viewer/",
    "clean": "del-cli android/build example/android/build example/android/app/build example/ios/build lib",
    "prepare": "bob build && cd web-viewer && yarn install && yarn build && cd ..",
    "release": "release-it",
    "start-server": "node server/debug-server.js",
    "track-net-vision-logs": "adb logcat -c && adb logcat | grep NetVision"
  },
  "keywords": [
    "react-native",
    "network",
    "debugger",
    "interceptor",
    "devtools",
    "websocket",
    "network-monitoring"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/omeratt/rn-net-vision.git"
  },
  "author": "OmerAttias <omeratt1405@gmail.com> (https://github.com/omeratt)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/omeratt/rn-net-vision/issues"
  },
  "homepage": "https://github.com/omeratt/rn-net-vision#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "dependencies": {
    "ws": "^8.0.0"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^19.6.0",
    "@eslint/compat": "^1.2.7",
    "@eslint/eslintrc": "^3.3.0",
    "@eslint/js": "^9.22.0",
    "@evilmartians/lefthook": "^1.5.0",
    "@react-native/eslint-config": "^0.78.0",
    "@release-it/conventional-changelog": "^9.0.2",
    "@types/jest": "^29.5.5",
    "@types/react": "^19.0.0",
    "commitlint": "^19.6.1",
    "del-cli": "^5.1.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.3",
    "jest": "^29.7.0",
    "metro-react-native-babel-preset": "^0.77.0",
    "open": "^10.1.1",
    "prettier": "^3.0.3",
    "react": "19.0.0",
    "react-native": "0.78.2",
    "react-native-builder-bob": "0.18.0",
    "release-it": "^17.10.0",
    "turbo": "^1.10.7",
    "typescript": "^5.2.2"
  },
  "react-native": {
    "module": "src/index.ts"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "workspaces": [
    "example"
  ],
  "packageManager": "yarn@3.6.1",
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/example/node_modules",
      "<rootDir>/lib/"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "body-max-line-length": [
        0,
        "always",
        "Infinity"
      ],
      "header-max-length": [
        2,
        "always",
        "Infinity"
      ],
      "subject-case": [
        0
      ]
    }
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": {
          "name": "angular"
        }
      }
    }
  },
  "prettier": {
    "quoteProps": "consistent",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      [
        "module",
        {
          "esm": true
        }
      ],
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "create-react-native-library": {
    "type": "legacy-module",
    "languages": "kotlin-swift",
    "version": "0.49.7"
  }
}
